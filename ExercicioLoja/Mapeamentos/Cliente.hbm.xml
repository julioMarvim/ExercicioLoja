<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly ="ExercicioLoja"
                   namespace ="ExercicioLoja.Entidades">
<class name="Cliente">
  <id name="Id">
    <generator class="identity"></generator>
  </id>
  <property name="Nome"></property>
  <property name="Telefone"></property>
  <property name="Endereco"></property>
  <property name="LimiteDeCredito"></property>
    <property name="Filiacao"></property>
    <property name="Status"></property>
    <property name="TipoDeCliente"></property>
</class>
</hibernate-mapping>
